{
  "name": "snapz-app",
  "version": "1.0.0",
  "description": "A React App which queries the Flickr API",
  "main": "index.js",
  "author": "Kay Wood",
  "scripts": {
    "start": "webpack-dev-server --open",
    "build": "webpack",
    "lint": "eslint . --ext .js, .jsx",
    "unit": "jest",
    "unit:ci": "jest --maxWorkers=4",
    "test": "yarn lint && yarn unit",
    "test:ci": "yarn lint && yarn unit:ci"
  },
  "dependencies": {
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "prop-types": "^15.6.0",
    "react-router-dom": "^4.2.2",
    "nebula-react": "^0.8.1",
    "react-html-parser": "^2.0.1",
    "react-icons": "^2.2.7"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-root-import": "^5.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.29.0",
    "jest": "^21.2.1",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "stylelint": "^8.2.0",
    "stylelint-config-nebula": "^0.2.0",
    "stylelint-webpack-plugin": "^0.9.0",
    "svg-sprite-loader": "^3.4.1",
    "url-loader": "^0.6.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "rootDir": "./src",
    "resetMocks": true,
    "resetModules": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}"
    ]
  },
  "keywords": [
    "Snapz",
    "image",
    "app",
    "photos"
  ]
}
